{
  "name": "relascope-insight",
  "version": "0.1.0",
  "description": "A powerful desktop application for personal information management and analysis using LLM",
  "main": "build/main/main/index.js",
  "scripts": {
    "start": "electron .",
    "dev": "webpack serve --config config/webpack/webpack.dev.js",
    "build": "webpack --config config/webpack/webpack.prod.js",
    "build:main": "tsc -p tsconfig.main.json && node scripts/copy-sql-files.js",
    "build:preload": "tsc -p tsconfig.main.json",
    "build:all": "npm run build && npm run build:main",
    "electron:dev": "npm run build:main && cross-env NODE_ENV=development electron . --trace-warnings",
    "electron:prod": "npm run build:main && cross-env NODE_ENV=production electron .",
    "app:dev": "concurrently -k \"npm run dev\" \"node scripts/wait.js && npm run electron:dev\"",
    "app:prod": "concurrently \"npm run build\" \"npm run electron:prod\"",
    "package": "npm run build && npm run build:main && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "llm",
    "analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "axios": "1.6.7",
    "electron-log": "5.0.0",
    "electron-store": "3.0.0",
    "framer-motion": "^7.10.3",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^8.0.5",
    "i18next-http-backend": "^3.0.2",
    "marked": "11.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.56.2",
    "react-hot-toast": "2.4.1",
    "react-i18next": "^13.5.0",
    "react-icons": "^5.5.0",
    "react-refresh": "0.14.0",
    "react-router-dom": "^6.30.0",
    "recharts": "2.5.0",
    "redux-toolkit": "1.1.2",
    "sqlite": "^5.1.1",
    "sqlite3": "5.1.7",
    "styled-components": "6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.0",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@testing-library/react": "14.1.2",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "babel-loader": "9.1.3",
    "buffer": "^6.0.3",
    "concurrently": "8.2.2",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "css-loader": "6.10.0",
    "electron": "28.2.7",
    "electron-builder": "24.0.0",
    "electron-forge": "5.2.4",
    "electron-rebuild": "3.2.0",
    "eslint": "8.38.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.0",
    "jest": "29.7.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "2.8.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "style-loader": "3.3.0",
    "ts-jest": "29.1.1",
    "typescript": "4.9.5",
    "util": "^0.12.5",
    "webpack": "5.91.0",
    "webpack-bundle-analyzer": "4.9.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.10.0"
  }
}
